<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">


<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:gxt='urn:import:com.extjs.gxt.ui.client.widget' xmlns:tree="urn:import:com.extjs.gxt.ui.client.widget.treepanel">

	<gxt:Viewport layout="FitLayout">
		<gxt:ContentPanel layout="FitLayout" heading="GXT UiBinder Explorer Demo" collapsible="false">
			<gxt:LayoutContainer layout="BorderLayout">
				<gxt:west size="218" split="true" collapsible="true" margins="5">
					<gxt:ContentPanel heading="Navigation" ui:field="navigationContainer">
					</gxt:ContentPanel>
				</gxt:west>
				<gxt:center margins="5,5,5,0">
					<gxt:ContentPanel headerVisible="false" layout="FitLayout"  ui:field="contentContainer"  />
				</gxt:center>
			</gxt:LayoutContainer>
		</gxt:ContentPanel>
	</gxt:Viewport>


</ui:UiBinder>